[ -f "$HOME/.zsh/zsh-autosuggestions/zsh-autosuggestions.zsh" ] && source ~/.zsh/zsh-autosuggestions/zsh-autosuggestions.zsh
[ -f "$HOME/.zsh/zsh-syntax-highlighting/zsh-syntax-highlighting.plugin.zsh" ] && source ~/.zsh/zsh-syntax-highlighting/zsh-syntax-highlighting.plugin.zsh
[ -f "$HOME/.zsh/command-not-found/command-not-found.plugin.zsh" ] && source ~/.zsh/command-not-found/command-not-found.plugin.zsh
[ -f "$HOME/.zsh/zsh-completions/zsh-completions.plugin.zsh" ] && source ~/.zsh/zsh-completions/zsh-completions.plugin.zsh


# zfunctions
[ -d "$HOME/.zsh/zsh-completions/src/" ] && fpath+='$HOME/.zsh/zsh-completions/src/'
autoload -U compinit && compinit

[ -f "$HOME/.fzf.zsh" ] && source "$HOME/.fzf.zsh"

# Starship prompt
hash starship 2>/dev/null && eval "$(starship init zsh)"

MYLOC=`dirname $0`
if [ -d "$MYLOC/completions/" ];then
  for f in "$MYLOC/completions/"*;do
    source "$f"
  done
fi

# Starship prompt
hash starship 2>/dev/null && eval "$(starship init bash)"

[ -f "$MYLOC/completions/apt-fast" ] && source "$MYLOC/completions/apt-fast"
[ -f ~/.fzf.bash ] && source ~/.fzf.bash
